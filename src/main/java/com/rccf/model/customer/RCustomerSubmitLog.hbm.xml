<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rccf.model.customer.RCustomerSubmitLog" table="r_customer_submit_log" schema="rccf">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="customerId">
            <column name="customer_id" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="customerName">
            <column name="customer_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="customerPhone">
            <column name="customer_phone" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="customerLoanAmount">
            <column name="customer_loan_amount" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="customerLoanType">
            <column name="customer_loan_type" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="customerLoanTermMonth">
            <column name="customer_loan_term_month" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="customerLoanUsetype">
            <column name="customer_loan_usetype" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="customerLoanRepayment">
            <column name="customer_loan_repayment" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="customerLoanMonthrepay">
            <column name="customer_loan_monthrepay" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="customerRepayResource">
            <column name="customer_repay_resource" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="customerFiles">
            <column name="customer_files" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="submitSaleman">
            <column name="submit_saleman" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="submitDeputy">
            <column name="submit_deputy" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="submitDirector">
            <column name="submit_director" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="submitHouqi">
            <column name="submit_houqi" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="customerLoanTermDay">
            <column name="customer_loan_term_day" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>